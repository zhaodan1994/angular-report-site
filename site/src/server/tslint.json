{
    "defaultSeverity": "error",
    "extends": [
    "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
    "class-name": false,
    "max-line-length": false,
    "no-console" : false,
    "quotemark": [
    true,
    "single",
    "avoid-escape",
    "avoid-template"
    ],
    "variable-name": {
    "options": [
    "ban-keywords",
    "check-format"
    ]
    }
    },
    "rulesDirectory": []
   }