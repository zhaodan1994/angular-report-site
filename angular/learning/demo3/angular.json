{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1, 
  "newProjectRoot": "src/client",
  "projects": {
    "main": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "src/client/main",
      "sourceRoot": "src/client/main/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/main",
            "index": "src/client/main/src/index.html",
            "main": "src/client/main/src/main.ts",
            "polyfills": "src/client/main/src/polyfills.ts",
            "tsConfig": "src/client/main/tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/client/main/src/favicon.ico",
              "src/client/main/src/assets"
            ],
            "styles": [
              "src/client/main/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/client/main/src/environments/environment.ts",
                  "with": "src/client/main/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "main:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "main:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "main:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/client/main/src/test.ts",
            "polyfills": "src/client/main/src/polyfills.ts",
            "tsConfig": "src/client/main/tsconfig.spec.json",
            "karmaConfig": "src/client/main/karma.conf.js",
            "assets": [
              "src/client/main/src/favicon.ico",
              "src/client/main/src/assets"
            ],
            "styles": [
              "src/client/main/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/client/main/tsconfig.app.json",
              "src/client/main/tsconfig.spec.json",
              "src/client/main/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "src/client/main/e2e/protractor.conf.js",
            "devServerTarget": "main:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "main:serve:production"
            }
          }
        }
      }
    },
    "plot": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "src/client/plot",
      "sourceRoot": "src/client/plot/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/plot",
            "index": "src/client/plot/src/index.html",
            "main": "src/client/plot/src/main.ts",
            "polyfills": "src/client/plot/src/polyfills.ts",
            "tsConfig": "src/client/plot/tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/client/plot/src/favicon.ico",
              "src/client/plot/src/assets"
            ],
            "styles": [
              "src/client/plot/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/client/plot/src/environments/environment.ts",
                  "with": "src/client/plot/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "plot:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "plot:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "plot:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/client/plot/src/test.ts",
            "polyfills": "src/client/plot/src/polyfills.ts",
            "tsConfig": "src/client/plot/tsconfig.spec.json",
            "karmaConfig": "src/client/plot/karma.conf.js",
            "assets": [
              "src/client/plot/src/favicon.ico",
              "src/client/plot/src/assets"
            ],
            "styles": [
              "src/client/plot/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/client/plot/tsconfig.app.json",
              "src/client/plot/tsconfig.spec.json",
              "src/client/plot/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "src/client/plot/e2e/protractor.conf.js",
            "devServerTarget": "plot:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "plot:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "main"
}
